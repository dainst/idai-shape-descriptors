{
  "name": "idai-shape-descriptors",
  "version": "1.0.0",
  "description": "Shape descriptors for iDai.shapes",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "npm run build && jest --config jestconfig.json",
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dainst/idai-shape-descriptors.git"
  },
  "keywords": [],
  "author": "kihmono123",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dainst/idai-shape-descriptors/issues"
  },
  "homepage": "https://github.com/dainst/idai-shape-descriptors#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/ndarray": "^1.0.8",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jest": "^26.6.3",
    "rewire": "^5.0.0",
    "ts-jest": "^26.5.3",
    "typedoc": "^0.20.30",
    "typescript": "^4.2.3"
  },
  "files": [
    "lib/**/*"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/array-type": [
        "warn",
        {
          "default": "array"
        }
      ],
      "@typescript-eslint/semi": [
        "warn",
        "always"
      ],
      "@typescript-eslint/no-inferrable-types": 0,
      "max-len": [
        "warn",
        {
          "code": 120
        }
      ],
      "no-trailing-spaces": [
        "warn",
        {
          "skipBlankLines": true
        }
      ],
      "no-multiple-empty-lines": [
        "warn",
        {
          "max": 2
        }
      ],
      "quotes": [
        "warn",
        "single"
      ],
      "jsx-quotes": [
        "warn",
        "prefer-double"
      ],
      "object-curly-spacing": [
        "warn",
        "always"
      ],
      "no-multi-spaces": [
        "warn"
      ]
    }
  },
  "dependencies": {
    "@tensorflow/tfjs": "^3.2.0",
    "@types/ndarray-ops": "^1.2.0",
    "@types/ndarray-scratch": "^1.2.0",
    "ndarray": "^1.0.19",
    "ndarray-ops": "^1.2.2",
    "ndarray-pack": "^1.2.1",
    "ndarray-scratch": "^1.2.0"
  }
}
